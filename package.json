{
    "name": "openclaw-desktop",
    "version": "0.1.0",
    "description": "OpenClaw Desktop GUI — 可视化管理你的 AI 助手",
    "main": "dist-electron/main.js",
    "scripts": {
        "dev": "vite",
        "build": "vite build && electron-builder --config electron-builder.json5",
        "build:win": "vite build && electron-builder --win --config electron-builder.json5",
        "build:mac": "vite build && electron-builder --mac --config electron-builder.json5",
        "build:linux": "vite build && electron-builder --linux --config electron-builder.json5",
        "preview": "vite preview",
        "electron:dev": "concurrently \"vite\" \"wait-on http://localhost:5173 && electron .\"",
        "typecheck": "tsc --noEmit"
    },
    "author": "OpenClaw GUI Team",
    "license": "MIT",
    "dependencies": {
        "react": "^19.0.0",
        "react-dom": "^19.0.0",
        "react-router-dom": "^7.1.0",
        "zustand": "^5.0.0",
        "json5": "^2.2.3",
        "lucide-react": "^0.468.0"
    },
    "devDependencies": {
        "@types/react": "^19.0.0",
        "@types/react-dom": "^19.0.0",
        "@vitejs/plugin-react": "^4.3.0",
        "concurrently": "^9.1.0",
        "electron": "^34.0.0",
        "electron-builder": "^25.1.0",
        "typescript": "^5.7.0",
        "vite": "^6.0.0",
        "vite-plugin-electron": "^0.28.0",
        "vite-plugin-electron-renderer": "^0.14.0",
        "wait-on": "^8.0.0"
    }
}